                @Html.ValidationMessageFor(model => model.NumeroInscripcion, "", new { @class = "text-danger" })
            </div>
        </div>

        <h3 style="color: red;"> RUT DEBE SER INGRESADO SIN PUNTOS Y CON GUIÓN</h3>
        <div id="enajenantes-div">
            <h4 style="background: #2a2a2b; color: white; padding: 10px 20px;">ENAJENANTES</h4>
            <div id="enajenantes-table"></div>
            <button type="button" id="reactivity-add" onclick="addTableRowEnajenantes()">Añadir nueva fila</button>
        </div>
        <h4 style="background: #2a2a2b; color: white; padding: 10px 20px;">ADQUIRIENTES</h4>
        <div id="adquirientes-table"></div>
        <button type="button" id="reactivity-add" onclick="addTableRowAdquirientes()">Añadir nueva fila</button>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="hidden" id="receivedEnajenantes" name="receivedEnajenantes">
                <input type="hidden" id="receivedAdquirientes" name="receivedAdquirientes">
                <button type="submit" value="Crear" class="btn btn-default" onclick="submitForm()">Subir</button>
            </div>
        </div>
    </div>
}

