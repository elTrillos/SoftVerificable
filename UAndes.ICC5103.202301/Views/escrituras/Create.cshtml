<script>
    const cneDropdown = document.getElementById("cne-dropdown");
    const divToHide = document.getElementById("enajenantes-div");
    cneDropdown.addEventListener("change", function () {
        if (cneDropdown.value === "regularizacion") {
            divToHide.style.display = "none";
        } else {
            divToHide.style.display = "block";
        }
    });
</script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script type="text/javascript" src="https://unpkg.com/tabulator-tables@5.4.4/dist/js/tabulator.min.js"></script>
<script>
    let enajenantesJson;
    let adquirientesJson;

    function addTableRowEnajenantes() {
        var table = Tabulator.findTable("#enajenantes-table")[0]
        var rows = table.getRows();
        table.addRow({ item: rows.length + 1 });
    }

    function addTableRowAdquirientes() {
        var table = Tabulator.findTable("#adquirientes-table")[0]
        var rows = table.getRows();
        table.addRow({ item: rows.length + 1 });
    }

    $(document).ready(function () {
        const tableDataEnajenantes = [
            {
                item: 1,
                rut: "",
                porcentajeDerecho: 0,
                porcentajeDerechoNoAcreditado: false
            },
        ];

        const tableDataAdquirientes = [
            {
                item: 1,
                rut: "",
                porcentajeDerecho: 0,
                porcentajeDerechoNoAcreditado: false,
            },
        ];

        var tableEnajenantes = new Tabulator("#enajenantes-table", {
            height: "311px",
            layout: "fitColumns",
            resizableColumnFit: true,
            responsiveLayout: "hide",
            reactiveData: true, //turn on data reactivity
            data: tableDataEnajenantes, //load data into table
            columns: [
                {
                    title: "RUT",
                    field: "rut",
                    editor: true,
                    editorParams: {
                        elementAttributes: {
                            maxlength: "10", //set the maximum character length of the input element to 10 characters
                        },
                        selectContents: true,
                        verticalNavigation: "table", //up and down arrow keys navigate away from cell without changing value
                    },
                },
                {
                    title: "% de derecho",
                    field: "porcentajeDerecho",
                    sorter: "number",
                    editor: true,
                    width: 150,
                    validator: ["required", "min:0", "max:100"],
                    editorParams: {
                        min: 0,
                        max: 100,
                        step: 10,
                        elementAttributes: {
                            maxlength: "10", //set the maximum character length of the input element to 10 characters
                        },
                        mask: "999",
                        selectContents: true,
                        verticalNavigation: "table", //up and down arrow keys navigate away from cell without changing value
                    },
                },
                {
                    title: "% de derecho no acreditado",
                    field: "porcentajeDerechoNoAcreditado",
                    hozAlign: "center",
                    editor: true,
                    formatter: "tickCross",
                    width: 230,
                },
            ],
        });

        tableEnajenantes.on("dataChanged", function (row) {
            var tableEnajenantes = Tabulator.findTable("#enajenantes-table")[0]
            var array = tableEnajenantes.getData();
            var enajenanteJsonHiddenInput = document.querySelector("#receivedEnajenantes");
            enajenantesJson = JSON.stringify(array);
            enajenanteJsonHiddenInput.value = enajenantesJson;
        }
        );

        var tableAdquirientes = new Tabulator("#adquirientes-table", {
            height: "311px",
            layout: "fitColumns",
            resizableColumnFit: true,
            responsiveLayout: "hide",
            reactiveData: true,
            data: tableDataAdquirientes,
            columns: [
                {
                    title: "RUT",
                    field: "rut",
                    editor: true,
                    editorParams: {
                        elementAttributes: {
                            maxlength: "10",
                        },
                        selectContents: true,
                        verticalNavigation: "table",
                    },
                },
                {
                    title: "% de derecho",
                    field: "porcentajeDerecho",
                    sorter: "number",
                    editor: true,
                    width: 150,
                    validator: ["required", "min:0", "max:100"],
                    editorParams: {
                        min: 0,
                        max: 100,
                        step: 10,
                        elementAttributes: {
                            maxlength: "10",
                        },
                        mask: "999",
                        selectContents: true,
                        verticalNavigation: "table",
                    },
                },
                {
                    title: "% de derecho no acreditado",
                    field: "porcentajeDerechoNoAcreditado",
                    hozAlign: "center",
                    editor: true,
                    formatter: "tickCross",
                    width: 230,
                },
            ],
        });

        tableAdquirientes.on("dataChanged", function (row) {
            var tableAdquirientes = Tabulator.findTable("#adquirientes-table")[0];
            var array = tableAdquirientes.getData();
            adquirientesJson = JSON.stringify(array);
            var adquirientesJsonHiddenInput = document.querySelector("#receivedAdquirientes");
            adquirientesJsonHiddenInput.value = adquirientesJson;
        });
    });
</script>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
